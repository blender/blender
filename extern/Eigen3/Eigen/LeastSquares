  * #include <Eigen/LeastSquares>
  * \endcode
  */

#include "src/Eigen2Support/LeastSquares.h"

#include "src/Core/util/ReenableStupidWarnings.h"

#endif // EIGEN2_SUPPORT

#endif // EIGEN_REGRESSION_MODULE_H
